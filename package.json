{
  "name": "slash-create-docs-bot",
  "version": "1.0.0",
  "description": "A template for slash-create",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "format": "biome format --write ./src/*",
    "lint": "biome lint ./src/*",
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "bun ./src/index.ts",
    "lint:fix": "npx eslint --ext .ts ./src --fix"
  },
  "dependencies": {
    "@types/mathjax": "^0.0.40",
    "chrono-node": "^2.7.7",
    "convert-units": "^3.0.0-beta.7",
    "fuzzy": "^0.1.3",
    "katex": "^0.16.25",
    "slash-create": "^6.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.14",
    "eventemitter3": "^5.0.1",
    "lodash.isequal": "^4.5.0",
    "slash-up": "^1.4.2",
    "ts-node": "^10.9.2",
    "tweetnacl": "^1.0.3",
    "typescript": "5.4.1-rc",
    "undici": "^6.21.0"
  }
}
